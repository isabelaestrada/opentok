<p id="notice"><%= notice %></p>

<h1>Visits</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Geocoder</th>
      <th>Device</th>
      <th>Backtrace</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visits.each do |visit| %>
      <tr>
        <td><%= visit.location.inspect if visit.location.present? %></td>
        <td><%= visit.geocoder.inspect if visit.geocoder.present? %></td>
        <td><%= visit.device.inspect if visit.device.present? %></td>
        <td><%= visit.backtrace.inspect if visit.backtrace.present? %></td>
        <td><%= link_to 'Show', visit %></td>
        <td><%= link_to 'Edit', edit_visit_path(visit) %></td>
        <td><%= link_to 'Destroy', visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit', new_visit_path %>
